# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /test                       controllers.TaskController.index



GET     /CountAsBs                  controllers.Application.CountAsBs
GET     /NaiveBayesclassifier       controllers.Application.NaiveBayes
GET     /Statistics                 controllers.Application.statistics
GET     /Basics                     controllers.Application.basics
GET     /DecisionTree               controllers.Application.DecisionTree
GET     /FeatureAnalysis           controllers.Application.FeatureAnalysis






# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
